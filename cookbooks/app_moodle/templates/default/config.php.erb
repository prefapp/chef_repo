<?php  // Moodle configuration file

unset($CFG);
global $CFG;
$CFG = new stdClass();

$CFG->dbtype    = '<%=@app['db_type']%>';
$CFG->dblibrary = 'native';
$CFG->dbhost    = '<%=@app['db_host']%>';
$CFG->dbname    = '<%=@app['db_name']%>';
$CFG->dbuser    = '<%=@app['db_user']%>';
$CFG->dbpass    = '<%=@app['db_password']%>';
$CFG->prefix    = 'mdl_';
$CFG->dboptions = array (
  'dbpersist' => 0,
  'dbport' => '', #default port
  'dbsocket' => false
);

$CFG->wwwroot   = 'http://<%=@app['domain']%>';
$CFG->dataroot  = '<%=@app['datadir']%>';
$CFG->admin     = 'admin';

$CFG->directorypermissions = 0777;

require_once(dirname(__FILE__) . '/lib/setup.php');

// There is no php closing tag in this file,
// it is intentional because it prevents trailing whitespace problems!
